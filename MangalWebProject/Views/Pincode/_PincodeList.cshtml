@model IList<MangalWebProject.Models.Entity.Mst_PinCode>
@using MangalWebProject.Models.Entity
<div class="modal-content">
    <div class="modal-body" style="height:inherit; overflow:auto;">
        <div class="panel-body panel-no-padding" style="height:inherit; overflow:auto;">
            <div id="demo">
                <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-hover table-fixed-header m0" id="example">
                    <thead>
                        <tr>
                            <th>Pin Code</th>
                            <th>Area Name</th>
                            <th>City</th>
                            <th>Zone</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (Mst_PinCode item in Model)
                        {
                            <tr class="gradeX" onclick='window.document.location="/Pincode/GetPincodeById?ID=@item.Pc_Id"'>
                                <td>@item.Pc_Desc</td>
                                <td>@item.Pc_AreaName</td>
                                <td>@item.Mst_City.Ct_Desc</td>
                                <td>@item.Mst_Zone.Zne_Desc</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div><!-- /.modal-content -->
